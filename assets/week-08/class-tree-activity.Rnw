<<>>=
set.seed(75)
n <- 16
x1 <- runif(n)
x2 <- runif(n)
group <- as.factor(sample(1:3, n, replace = TRUE))
df <- data.frame(x1, x2, group)
df[1, 2] <- .765
df[9, 1] <- .741

library(ggplot2)
ggplot(df, aes(x = x1, y = x2, col = group)) +
  geom_point(size = 3) +
  xlim(c(0, 1)) + ylim(c(0, 1))
@



